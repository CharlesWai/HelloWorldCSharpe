<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppForAndroid.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppForAndroid"
    xmlns:page="clr-namespace:MauiAppForAndroid.Pages"
    Shell.FlyoutBehavior="Flyout">

    <!--<ShellContent
        Title="HomePage"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->
    <ShellContent Title="SettingPage"
                  ContentTemplate="{DataTemplate page:SettingPage}"
                  Route="SettingPage"/>
    <ShellContent Title="DebugPage"
                  ContentTemplate="{DataTemplate page:DebugPage}"
                  Route="DebugPage"/>
    <ShellContent Title="AboutPage"
                  ContentTemplate="{DataTemplate page:AboutPage}"
                  Route="AboutPage"/>

</Shell>
